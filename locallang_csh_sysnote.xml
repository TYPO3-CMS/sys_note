    <data type="array">
        <languageKey index="default" type="array">
            <label index=".description">Internal reminder note placed on a the page.</label>
            <label index=".details">This provides a simple way to attach instructional notes to a page regarding backend related issues. Use it as you like for reminders, instructions etc. </label>
            <label index="_.seeAlso">pages
</label>
            <label index="category.description">Select a category which describes the character of the note.</label>
            <label index="subject.description">Enter a title for the note.</label>
            <label index="message.description">This is the note itself.</label>
            <label index="author.description">Enter your name. </label>
            <label index="email.description">Enter your email address.</label>
            <label index="personal.description">Check this, if you don't want other users to see this note.</label>
        </languageKey>
        <languageKey index="dk" type="array">
        </languageKey>
        <languageKey index="de" type="array">
            <label index=".description">Interne Erinnerungsnotiz auf einer Seite.</label>
            <label index=".details">Bietet eine einfache Möglichkeit, um interne Informationen und Anweisungen mit einer Seite zu verknüpfen.</label>
            <label index="category.description">Wähle eine die Notiz beschreibende Kategorie aus.</label>
            <label index="subject.description">Gib einen Titel für die Notiz ein.</label>
            <label index="message.description">Dies ist die eigentliche Notiz.</label>
            <label index="author.description">Gibt Deinen Namen ein.</label>
            <label index="email.description">Gib Deine EMail-Adresse ein.</label>
            <label index="personal.description">Notiz auch anderen Benutzern anzeigen.</label>
        </languageKey>
        <languageKey index="no" type="array">
        </languageKey>
